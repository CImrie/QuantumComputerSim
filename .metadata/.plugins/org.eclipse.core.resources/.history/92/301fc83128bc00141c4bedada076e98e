package core;

// @author Ben Crabbe
public class Qubit extends Complex {

	private Complex a, b;

	public Qubit(Complex a, Complex b){
		this.a = a;
		this.b = b;
	}

	@Override
	public String toString() {
		Complex a = this.get0();
		Complex b = this.get1();

		return "(" + a + ")| 0 > + (" + b + ")| 1 >";
	}
	
	public Complex get0(){
		return this.a;
	}
	
	public Complex get1(){
		return this.b;
	}

	
}
